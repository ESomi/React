nge} \r\n                                    value={this.state.phone}/>\r\n                            </div>\r\n                        </Fragment>\r\n                        \r\n                    ) : (\r\n                        <Fragment>\r\n                            <div><b>{name}</b></div>                \r\n                            <div><b>{phone}</b></div>\r\n                        </Fragment>\r\n                    )\r\n                }\r\n                <button onClick={this.handleRemove}>삭제</button>\r\n                <button onClick={this.handleToggleEdit}>\r\n                    { editing ? '적용' : '수정' }\r\n                </button>\r\n            </div>\r\n        );\r\n    }\r\n}\r\n\r\nexport default PhoneInfo;","C:\\Users\\some7\\react-tutorials\\contact-app\\src\\App.js",[],"C:\\Users\\some7\\react-tutorials\\contact-app\\src\\Contact.js",[],"C:\\Users\\some7\\react-tutorials\\contact-app\\src\\ContactInfo.js",[],"C:\\Users\\some7\\react-tutorials\\contact-app\\src\\ContactDetails.js",[],"C:\\Users\\some7\\react-tutorials\\contact-app\\src\\ContactCreate.js",[],"C:\\Users\\some7\\react-tutorials\\contact-app\\src\\ContactLight.js",[],{"ruleId":"54","replacedBy":"55"},{"ruleId":"56","replacedBy":"57"},{"ruleId":"54","replacedBy":"58"},{"ruleId":"56","replacedBy":"59"},{"ruleId":"60","severity":1,"message":"61","line":56,"column":30,"nodeType":"62","messageId":"63","endLine":56,"endColumn":32},"no-native-reassign",["64"],"no-negated-in-lhs",["65"],["64"],["65"],"no-unused-vars","'id' is assigned a value but never used.","Identifier","unusedVar","no-global-assign","no-unsafe-negation"]